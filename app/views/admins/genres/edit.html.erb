<h1>ジャンル編集</h1>
<%= form_with model: @genre, url: admins_genre_path(@genre), local:true do |f| %>
  <div>
    <%= f.label :ジャンル %>
    <%= f.text_field :name %>
    <%= f.radio_button :is_active, :true %>
    <%= f.label :is_active, "有効" %>
    <%= f.radio_button :is_active, :false %>
    <%= f.label :is_active, "無効" %>
    <%= f.submit '変更を保存する' %>
  </div>
<% end %>